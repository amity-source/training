@model List<Model.EF.Menu>
<div class="menu">
    <ul>
        @foreach (var item in Model)
        {
            <li><a href="@item.Link" target="@item.Target">@item.Text</a></li>
        }
        <li>@Html.ActionLink("Go to Admin", "Index", "Admin/Home")</li>
        <div class="clear"></div>
    </ul>
</div>
